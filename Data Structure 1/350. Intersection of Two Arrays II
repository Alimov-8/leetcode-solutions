class Solution:
    def intersect(self, nums1: List[int], nums2: List[int]) -> List[int]:
        st1 = set(nums1)
        st2 = set(nums2)
        nums = []
        for i in list(st1.intersection(st2)):
            if nums1.count(i) < nums2.count(i):
                for _ in range(nums1.count(i)):
                    nums.append(i)
            else:
                for _ in range(nums2.count(i)):
                    nums.append(i)
        return nums
