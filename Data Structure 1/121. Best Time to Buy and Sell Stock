class Solution:
    def maxProfit(self, prices: List[int]) -> int:
        maxprofit = 0
        if len(prices) == 0:
            return maxprofit
        
        l, r = 0, 1
        while(r <= len(prices) - 1):
            if prices[l] < prices[r]:
                maxprofit = max(maxprofit, prices[r] - prices[l])
                r += 1
            elif prices[l] >= prices[r]:
                l = r
                r += 1
                
        return maxprofit
